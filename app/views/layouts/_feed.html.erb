

<% activities.each do |activity| %>

          
          <div class="feed-container">
                
                <div class="feed-text">
                	
                    <%if activity.key == 'wishlist.create' %>
                    <span class="glyphicon glyphicon-star"></span>
                    <%= image_tag(activity.owner.largeimage, :class => "feed-thumbnail" , :title => "thumbnail-dp")%>
                    <div style="display:inline-block; vertical-align: top;">
                    <a class="feed-username" href=""><%= link_to activity.owner.name , root_url if activity.owner %></a>
                    <p class="feed-posttext">added wishlist item <%= link_to activity.trackable.link, root_url %> </p>
                      <p class="feed-time">at <%= activity.created_at %></p>
                    </div>
                    <%elsif activity.key == 'like.create' %>
                    <span class="glyphicon glyphicon-star"></span>
                    <%= image_tag(activity.owner.largeimage, :class => "feed-thumbnail" , :title => "thumbnail-dp")%>
                    <div style="display:inline-block; vertical-align: top;">
                    <a class="feed-username" href=""><%= link_to activity.owner.name , root_url if activity.owner %></a>
                    <p class="feed-posttext">liked wishlist item at </p>
                    <p class="feed-time"> <%= activity.created_at %></p>
                    </div>
                    <% else next %>
                    <%end%>

                </div>
              </div>
              
              <%end%>

                       
               
                                
                           
                