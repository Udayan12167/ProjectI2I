<% user = User.find_by_id(@user_id.id) %>

 <% user.wishlist.each do |t|%>

            <% if t.id==@object.to_i %>
            

				<% if t.liked_by?(current_user).to_s == "false" %>

    				<%= link_to '<span class="glyphicon glyphicon-thumbs-up"></span>'.html_safe, {controller:"wishlists", action:"vote" , wish_param: t.id ,my_params: @user_id.id} ,id: "sign_out", :remote => true %>
			<%end%>
              
			<% if t.liked_by?(current_user).to_s == "true" %>
				

    				<%= link_to '<span class="glyphicon glyphicon-thumbs-down"></span>'.html_safe, {controller:"wishlists", action:"unvote" , wish_param: t.id ,my_params: @user_id.id} ,id: "sign_out", :remote => true %>
			<%end%>
              
			

			<%= t.likers(User.all).count %>
	<%end%>
<%end%>
