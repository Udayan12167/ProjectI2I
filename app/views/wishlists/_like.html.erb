<% id=@object.to_i %>
 <% current_user.wishlist.each do |t|%>
            <% if t.id== id %>
				<% if t.liked_by?(current_user).to_s == "false" %>
    				<%= link_to '<i class="glyphicon glyphicon-thumbs-up"></i>'.html_safe, {controller:"wishlists", action:"vote" , my_param: "#{t.id}"} ,id: "sign_out",:remote => true%>
			<%end%>
              
			<% if t.liked_by?(current_user).to_s == "true" %>
  				<%= link_to '<i class="glyphicon glyphicon-thumbs-down"></i>'.html_safe, {controller:"wishlists", action:"unvote" , my_param: "#{t.id}"} ,id: "sign_out",:remote => true %>
<% user = User.find_by_id(@user_id.id) %>
 <% user.wishlist.each do |t|%>

            <% if t.id==@object.to_i %>
            
				<% if t.liked_by?(user).to_s == "false" %>
				
    				<%= link_to "like", {controller:"wishlists", action:"vote" , wish_param: t.id ,my_params: @user_id.id} ,id: "sign_out",:class => "btn btn-large btn-success" , :remote => true %>
			<%end%>
              
			<% if t.liked_by?(user).to_s == "true" %>
				
  				<%= link_to "unlike", {controller:"wishlists", action:"unvote" , wish_param: t.id,  my_params: @user_id.id} ,id: "sign_out",:class => "btn btn-large btn-success" , :remote =>true %>
			<%end%>

			<%= t.likers(User.all).count %>
	<%end%>
<%end%>

