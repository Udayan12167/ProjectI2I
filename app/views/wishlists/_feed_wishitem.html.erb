<div class="feed-container">
            <div class="feed-text">
                <%= image_tag(fimage(activity.owner_id), :class => "feed-thumbnail" , :title => "thumbnail-dp")%>
                <div style="display:inline-block; vertical-align: top;">
                    <p class="feed-time">at <%= time_ago_in_words(activity.created_at, include_seconds: true) %></p>
                    <a class="feed-username" href=""><%= link_to activity.name , root_url  %></a>
                    <p class="feed-posttext"> <%=activity.content %> </p>
                      
                </div>
                <%id1= activity.content.scan(/\d+/).first %>
                <%t = Wishlist.all.find_by_id(id1) %>
                <%user = User.find_by_id(activity.user_id) %>
                <% if t != nil %>
                    <li class="col-md-6">
                        <div class="thumbnail" style="padding: 0">
                            <div style="padding:4px;">
                                <div class="gift-info">  
                                    <%= image_tag('flash.jpg',class: "img-gift",alt:"300x200") %>        
                                    <h4>BC</h4>
                                    <h4><%=t.id%></h4>
                                    <p>My project description
                                        <%= user.wishlist.index(t) %>
     
                                        <i class="icon icon-map-marker"></i>
                                    </p>
                                </div>  
                            </div>
                            <div class="like_button">     
                            <% div_id="refresh_#{t.id}" %>
                            <div id="<%=div_id %>">
                                <% if t.liked_by?(current_user).to_s == "false" %>
                                    <%= link_to '<span class="glyphicon glyphicon-thumbs-up"></span>'.html_safe, {controller:"wishlists", action:"vote" , wish_param: "#{t.id}" ,my_params: activity.user_id} ,id: "sign_out", :remote => true %>
                                <%end%>
                 
                                <% if t.liked_by?(current_user).to_s == "true" %>
                                    <%= link_to '<span class="glyphicon glyphicon-thumbs-down"></span>'.html_safe, {controller:"wishlists", action:"unvote" , wish_param: "#{t.id}",  my_params: activity.user_id} ,id: "sign_out", :remote => true%>
                                <%end%>
               
                                <%= t.likers(User).count %>
                            </div>
                        </div>
                        </div>              
                    </li>
                <%end%>
                <span class="glyphicon glyphicon-heart feed-icon"></span>
            </div>
        </div>