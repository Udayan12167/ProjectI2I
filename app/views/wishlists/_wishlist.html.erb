          
          <% wish = Wishlist.all %>
          <% temp = my_params %>
          <% user = User.find_by_id(temp) %>
            <% wish.each do |t|%>
            <% if t.user_id == temp %>
            <li class="col-md-6">
              <%if user == current_user %>
              <%= link_to image_tag('remove.png',:class => "delete-icon"), {controller:"wishlists", action:"remove" , my_param: "#{t.id}" } %>
              <%end%>
              <div class="thumbnail" style="padding: 0">
                <div style="padding:4px;">
                 <div class="gift-info">   
                    <%= image_tag('flash.jpg',class: "img-gift",alt:"300x200") %>         
                    <h4>BC</h4>
                    <h4><%=t.id%></h4>
                     <p>My project description
                        <%= user.wishlist.index(t) %>

                         <i class="icon icon-map-marker"></i>
                       </p>
              </div>   
              </div>      
              <% div_id="refresh_#{t.id}" %>
              <div id="<%=div_id %>">
              <% if t.liked_by?(current_user).to_s == "false" %>

                <%= link_to '<span class="glyphicon glyphicon-thumbs-up"></span>'.html_safe, {controller:"wishlists", action:"vote" , wish_param: "#{t.id}" ,my_params: my_params} ,id: "sign_out", :remote => true %>

              <%end%>
              
              <% if t.liked_by?(current_user).to_s == "true" %>
              <%= link_to '<span class="glyphicon glyphicon-thumbs-down"></span>'.html_safe, {controller:"wishlists", action:"unvote" , wish_param: "#{t.id}",  my_params: my_params} ,id: "sign_out", :remote => true%>
              <%end%>
            
              <%= t.likers(User).count %>
            </div>
          </div>              
        </li>
      <%end%>
    <%end%>


