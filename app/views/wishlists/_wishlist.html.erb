          <% wish = Wishlist.all %>
          <% temp = my_params %>
          in wishlist
            <% wish.each do |t|%>
            <% if t.user_id == temp %>
            <li class="col-md-6">
              <%= link_to image_tag('remove.png',:class => "delete-icon"), {controller:"wishlists", action:"remove" , my_param: "#{t.id}" } %>
              <div class="thumbnail" style="padding: 0">
                <div style="padding:4px;">
                 <div class="gift-info">   
                    <%= image_tag('flash.jpg',class: "img-gift",alt:"300x200") %>         
                    <h4>BC</h4>
                    <h4><%=t.id%></h4>
                     <p>My project description
                        <%= current_user.wishlist.index(t) %>

                         <i class="icon icon-map-marker"></i>
                       </p>
              </div>
                    
                </div>      
              </div>
              <% div_id="refresh_#{t.id}" %>
              <div id="<%=div_id %>">
              <% if t.liked_by?(current_user).to_s == "false" %>

                <%= link_to "like", {controller:"wishlists", action:"vote" , my_param: "#{t.id}"} ,id: "sign_out",:class => "btn btn-large btn-success" ,:remote => true%>

              <%end%>
              
              <% if t.liked_by?(current_user).to_s == "true" %>
              <%= link_to "unlike", {controller:"wishlists", action:"unvote" , my_param: "#{t.id}"} ,id: "sign_out",:class => "btn btn-large btn-success",:remote => true %>
              <%end%>
            
              <%= t.likers(User).count %>
            </div>
              
            </li>
            <%end%>
            <%end%>


