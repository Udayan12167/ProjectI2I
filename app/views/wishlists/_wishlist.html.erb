

            <% current_user.wishlist.each do |t|%>
            <% if t.id!= nil %>
            <li class="col-md-6">
              <%= button_to "remove", {controller:"wishlists", action:"remove" , my_param: "#{t.id}" } %>
              <div class="thumbnail" style="padding: 0">
                <div style="padding:4px;">
                 <div class="gift-info">   
                    <%= image_tag('flash.jpg',class: "img-gift",alt:"300x200") %>         
                    <h4>BC</h4>
                    <h4><%=t.id%></h4>
                     <p>My project description
                        <%= t.id %>
                         <i class="icon icon-map-marker"></i>
                       </p>
              </div>
                    
                </div>      
              </div>

              <% if t.liked_by?(current_user).to_s == "false" %>
                <%= button_to "like", {controller:"wishlists", action:"vote" , my_param: "#{t.id}" , method: :post} ,id: "sign_out",:class => "btn btn-large btn-success" %>
              <%end%>
              
              <% if t.liked_by?(current_user).to_s == "true" %>
              <%= link_to "unlike", {controller:"wishlists", action:"unvote" , my_param: "#{t.id}"} ,id: "sign_out",:class => "btn btn-large btn-success" %>
              <%end%>
              <%= t.likers(User).count %>
              
            </li>
            <%end%>
            <%end%>


